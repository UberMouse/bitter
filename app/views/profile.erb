<h1><%= @username %></h1>
<img src="<%= @gravatar_url %>" />
<% @tweets.each do |tweet| %>
    <%= erb :'_tweet', :locals => { text: tweet[0], user_name: tweet[1], created_at: tweet[2], tweet_id: tweet[3] } %>
<% end %>

<div>  <a href="/users/<%=@username%>/honchos"><p>Honchos:<%= @honcho_count %></p></a> </div>

<div>  <a href="/users/<%=@username%>/stalkers"><p>Stalkers:<%= @stalkers_count %></p></a> </div>

<a href="/stalk/<%= @userid %>"><%= @stalk_button_message %></a>

